{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "sourceMap": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {
            "src/*": ["src/*"],
            "assets/*": ["assets/*"],
            "components/*": ["src/components/*"],
            "css/*": ["src/css/*"],
            "img/*": ["src/img/*"],
        }
    }
}